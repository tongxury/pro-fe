{
  "name": "trade-master",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "homepage": "https://telegram-mini-apps.github.io/reactjs-tsdk-template",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite --mode development",
    "start": "vite --mode staging",
    "build:staging": "rm -rf dist && tsc --noEmit && vite build --mode staging",
    "build:production": "rm -rf dist && tsc --noEmit && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "pnpm run build",
    "iconfont": "iconfont-h5"
  },
